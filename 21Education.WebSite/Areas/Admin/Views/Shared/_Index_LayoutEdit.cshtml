<!DOCTYPE html>
<html>
<head>
    <title>Main</title>
    @Styles.Render("~/Areas/Content/themes/blue/css")
    <script src="@Url.Content("~/Areas/Content/Scripts/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/Content/Scripts/easyui/plugins/jquery.form.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/Content/Scripts/jquery.easyui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/Content/Scripts/jquery.easyui.plus.js")"></script>
    @Styles.Render("~/Areas/Content/css")
    @Styles.Render("~/bundles/datepicker-style")
    <script src="@Url.Content("~/Areas/Content/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/Content/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Areas/Content/Scripts/jquery.unobtrusive-ajax.min.js")" type="text/javascript"></script>
    
</head>

<body style="width:800px;">

    @RenderBody()

    @Scripts.Render("~/bundles/datepicker-script")
    @Scripts.Render("~/bundles/jquery-fileupload")
    <script>
        $("[for$=review]:contains(预览)").hover(function () {
            $(this).children("img").css("display","block");
        },
        function () {
            $(this).children("img").css("display", "none");
        }
        )
    </script>
</body>
</html>